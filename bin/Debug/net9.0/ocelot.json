{
  "Routes": [
    {
      "DownstreamPathTemplate": "/swagger/v1/swagger.json",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "bill-pay-api.onrender.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/swagger/v1/swagger.json",
      "UpstreamHttpMethod": [ "GET" ],
      "SwaggerKey": "mobileProvider"
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "bill-pay-api.onrender.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "api/v1/MobileProviderApp/query-bill",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "bill-pay-api.onrender.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/v1/MobileProviderApp/query-bill",
      "UpstreamHttpMethod": [ "GET" ],
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "ClientIdHeader": "X-SubscriberNo",
        "Period": "1d",
        "Limit": 3
      }
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "mobileProvider",
      "Config": [
        {
          "Name": "Mobile Provider API",
          "Version": "v1",
          "Url": "https://bill-pay-api.onrender.com/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://mobile-billing-gateway-2kqo.onrender.com"
  }
}
